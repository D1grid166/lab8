begin
  writeln('Который час');
  var n:=ReadInteger;
  assert((n>0) and (n<=23));
  case n of
    4..10: print('Доброе утро, мир!');
    11..16: print('Добрый день, мир!');
    17..23: print('Добрый вечер, мир!');
    else println('Доброй ночи, мир!');
  end;
end.

